{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "ignoreDeprecations": "5.0",
    "paths": {
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@services/*": ["src/service/*"],
      "@constants/*": ["src/constants/*"],
      "@layouts/*": ["src/layouts/*"]
    }
  }
}
